import java.util.*;
public class Palindrome_name{
public static void main(String args[]){
Scanner sc=new Scanner(System.in);
int i,j,l,l1;
String st,st1="";
char ch=0,ch1=0,ch2=0;
System.out.println("Enter a string");
st=sc.next();
l=st.length();
for(i=0;i<l;i++){
ch=st.charAt(i);
if(ch==' '){
st1=st.substring(p,i);
p=i+1;
l1=st1.length();
for(j=0;j<l1;j++){
ch1=st1.charAt(i);
ch2=st1.charAt(l-1);
if(ch1==ch2)
System.out.println(st1);
}
}
else
st1=st1+ch;
}
}
